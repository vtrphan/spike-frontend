{
  "version": 2,
  "cli": {
    "defaultCollection": "@nrwl/next"
  },
  "generators": {
    "@nrwl/react": {
      "application": {
        "babel": true
      }
    },
    "@nrwl/next": {
      "application": {
        "style": "@emotion/styled",
        "linter": "eslint"
      }
    }
  },
  "defaultProject": "spike-frontend",
  "projects": {
    "spike-frontend": {
      "root": "apps/spike-frontend",
      "sourceRoot": "apps/spike-frontend",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/next:build",
          "outputs": [
            "{options.outputPath}"
          ],
          "options": {
            "root": "apps/spike-frontend",
            "outputPath": "dist/apps/spike-frontend"
          },
          "configurations": {
            "production": {}
          }
        },
        "serve": {
          "executor": "@nrwl/next:server",
          "options": {
            "buildTarget": "spike-frontend:build",
            "dev": true
          },
          "configurations": {
            "production": {
              "buildTarget": "spike-frontend:build:production",
              "dev": false
            }
          }
        },
        "export": {
          "executor": "@nrwl/next:export",
          "options": {
            "buildTarget": "spike-frontend:build:production"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/spike-frontend/**/*.{ts,tsx,js,jsx}"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/apps/spike-frontend"
          ],
          "options": {
            "jestConfig": "apps/spike-frontend/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "spike-frontend-e2e": {
      "root": "apps/spike-frontend-e2e",
      "sourceRoot": "apps/spike-frontend-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/spike-frontend-e2e/cypress.json",
            "tsConfig": "apps/spike-frontend-e2e/tsconfig.e2e.json",
            "devServerTarget": "spike-frontend:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "spike-frontend:serve:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/spike-frontend-e2e/**/*.{js,ts}"
            ]
          }
        }
      }
    },
    "spike-fe-ionic": {
      "root": "apps/spike-fe-ionic",
      "sourceRoot": "apps/spike-fe-ionic/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/web:build",
          "outputs": [
            "{options.outputPath}"
          ],
          "options": {
            "outputPath": "dist/apps/spike-fe-ionic",
            "index": "apps/spike-fe-ionic/src/index.html",
            "main": "apps/spike-fe-ionic/src/main.tsx",
            "polyfills": "apps/spike-fe-ionic/src/polyfills.ts",
            "tsConfig": "apps/spike-fe-ionic/tsconfig.app.json",
            "assets": [
              "apps/spike-fe-ionic/src/assets",
              "apps/spike-fe-ionic/src/manifest.json"
            ],
            "styles": [
              "apps/spike-fe-ionic/src/styles.css"
            ],
            "scripts": [],
            "webpackConfig": "@nrwl/react/plugins/webpack"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/spike-fe-ionic/src/environments/environment.ts",
                  "with": "apps/spike-fe-ionic/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/web:dev-server",
          "options": {
            "buildTarget": "spike-fe-ionic:build"
          },
          "configurations": {
            "production": {
              "buildTarget": "spike-fe-ionic:build:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/spike-fe-ionic/**/*.{ts,tsx,js,jsx}"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/apps/spike-fe-ionic"
          ],
          "options": {
            "jestConfig": "apps/spike-fe-ionic/jest.config.js",
            "passWithNoTests": true
          }
        },
        "cap": {
          "executor": "@nxtend/capacitor:cap",
          "options": {
            "cmd": "--help"
          }
        },
        "add": {
          "executor": "@nxtend/capacitor:cap",
          "options": {
            "cmd": "add",
            "packageInstall": true
          },
          "configurations": {
            "ios": {
              "cmd": "add ios"
            },
            "android": {
              "cmd": "add android"
            }
          }
        },
        "copy": {
          "executor": "@nxtend/capacitor:cap",
          "options": {
            "cmd": "copy",
            "packageInstall": false
          },
          "configurations": {
            "ios": {
              "cmd": "copy ios"
            },
            "android": {
              "cmd": "copy android"
            }
          }
        },
        "open": {
          "executor": "@nxtend/capacitor:cap",
          "options": {
            "cmd": "open",
            "packageInstall": false
          },
          "configurations": {
            "ios": {
              "cmd": "open ios"
            },
            "android": {
              "cmd": "open android"
            }
          }
        },
        "sync": {
          "executor": "@nxtend/capacitor:cap",
          "options": {
            "cmd": "sync",
            "packageInstall": true
          },
          "configurations": {
            "ios": {
              "cmd": "sync ios"
            },
            "android": {
              "cmd": "sync android"
            }
          }
        },
        "update": {
          "executor": "@nxtend/capacitor:cap",
          "options": {
            "cmd": "update",
            "packageInstall": true
          },
          "configurations": {
            "ios": {
              "cmd": "update ios"
            },
            "android": {
              "cmd": "update android"
            }
          }
        }
      }
    },
    "spike-fe-ionic-e2e": {
      "root": "apps/spike-fe-ionic-e2e",
      "sourceRoot": "apps/spike-fe-ionic-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/spike-fe-ionic-e2e/cypress.json",
            "tsConfig": "apps/spike-fe-ionic-e2e/tsconfig.e2e.json",
            "devServerTarget": "spike-fe-ionic:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "spike-fe-ionic:serve:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/spike-fe-ionic-e2e/**/*.{js,ts}"
            ]
          }
        }
      }
    },
    "ui": {
      "root": "libs/ui",
      "sourceRoot": "libs/ui/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/ui/**/*.{ts,tsx,js,jsx}"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/libs/ui"
          ],
          "options": {
            "jestConfig": "libs/ui/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    }
  }
}